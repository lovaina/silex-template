{
  "name": "actualmed/um-console",
  "description": "Subida de estudios mediante un directorio y modificar información según cita para luego enviarlo al pacs",
  "version": "1.0",
  "authors": [
    {
      "name": "Actualmed",
      "email": "soporte@actualmed.com",
      "homepage": "http://www.actualmed.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "App\\": "app",
      "UmConsole\\": "src/"
    },
    "files": [
      "src/tools/files.php",
      "src/tools/dicom.php"
    ]

  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "require": {
    "silex/silex": "~2.0",
    "guzzlehttp/guzzle": "~5.3",
    "monolog/monolog": "^1.22",
    "symfony/finder": "^3.2",
    "symfony/var-dumper": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "symfony/browser-kit": "^3.2",
    "symfony/css-selector": "^3.2",
    "phpunit/dbunit": "^2.0"
  }
}
